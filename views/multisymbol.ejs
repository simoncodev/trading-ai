<!DOCTYPE html>
<html lang="it">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Multi-Symbol Trading Dashboard</title>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Space+Mono:wght@400;700&family=Orbitron:wght@400;700;900&display=swap');
        
        * { margin: 0; padding: 0; box-sizing: border-box; }
        
        :root {
            --bg-dark: #0a0a0f;
            --bg-card: #12121a;
            --bg-card-hover: #1a1a25;
            --border: #2a2a3a;
            --text: #e0e0e0;
            --text-dim: #888;
            --green: #00ff88;
            --green-dim: #00aa55;
            --red: #ff4455;
            --red-dim: #aa2233;
            --blue: #00aaff;
            --yellow: #ffaa00;
            --purple: #aa55ff;
            --btc: #f7931a;
            --eth: #627eea;
            --sol: #00ffa3;
            --doge: #c3a634;
            --xrp: #23292f;
        }
        
        body {
            font-family: 'Space Mono', monospace;
            background: var(--bg-dark);
            color: var(--text);
            min-height: 100vh;
            overflow-x: hidden;
        }

        /* HEADER */
        .header {
            background: linear-gradient(180deg, #15151f 0%, var(--bg-dark) 100%);
            border-bottom: 1px solid var(--border);
            padding: 12px 25px;
            display: flex;
            justify-content: space-between;
            align-items: center;
            position: sticky;
            top: 0;
            z-index: 100;
        }

        .logo {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .logo-icon {
            width: 40px;
            height: 40px;
            background: linear-gradient(135deg, var(--green) 0%, var(--blue) 100%);
            border-radius: 10px;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 20px;
        }

        .logo-text {
            font-family: 'Orbitron', sans-serif;
            font-size: 18px;
            font-weight: 700;
            background: linear-gradient(90deg, var(--green), var(--blue));
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        .logo-subtitle {
            font-size: 10px;
            color: var(--text-dim);
            letter-spacing: 2px;
        }

        .nav-links {
            display: flex;
            gap: 15px;
        }

        .nav-link {
            padding: 8px 16px;
            background: var(--bg-card);
            border: 1px solid var(--border);
            border-radius: 8px;
            color: var(--text);
            text-decoration: none;
            font-size: 12px;
            transition: all 0.3s;
        }

        .nav-link:hover {
            border-color: var(--blue);
            background: var(--bg-card-hover);
        }

        .nav-link.active {
            border-color: var(--green);
            color: var(--green);
        }

        /* TABS */
        .tabs-container {
            display: flex;
            gap: 5px;
            padding: 15px 20px;
            background: var(--bg-card);
            border-bottom: 1px solid var(--border);
            overflow-x: auto;
        }

        .tab {
            padding: 12px 25px;
            background: transparent;
            border: 2px solid var(--border);
            border-radius: 10px;
            color: var(--text-dim);
            font-family: 'Orbitron', sans-serif;
            font-size: 14px;
            font-weight: 700;
            cursor: pointer;
            transition: all 0.3s;
            display: flex;
            align-items: center;
            gap: 10px;
            white-space: nowrap;
        }

        .tab:hover {
            border-color: var(--blue);
            color: var(--text);
        }

        .tab.active {
            background: linear-gradient(135deg, rgba(0,255,136,0.15), rgba(0,170,255,0.15));
            border-color: var(--green);
            color: var(--green);
        }

        .tab-icon {
            width: 24px;
            height: 24px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 12px;
            font-weight: 900;
        }

        .tab-icon.btc { background: var(--btc); color: #000; }
        .tab-icon.eth { background: var(--eth); color: #fff; }
        .tab-icon.sol { background: var(--sol); color: #000; }
        .tab-icon.doge { background: var(--doge); color: #000; }
        .tab-icon.xrp { background: #23292f; color: #fff; border: 2px solid #fff; }

        .tab-signal {
            padding: 3px 8px;
            border-radius: 5px;
            font-size: 10px;
            font-weight: 900;
        }

        .tab-signal.buy { background: rgba(0,255,136,0.3); color: var(--green); }
        .tab-signal.sell { background: rgba(255,68,85,0.3); color: var(--red); }
        .tab-signal.wait { background: rgba(136,136,136,0.3); color: var(--text-dim); }

        /* MAIN CONTAINER */
        .container {
            max-width: 1900px;
            margin: 0 auto;
            padding: 20px;
        }

        /* SYMBOL PANEL */
        .symbol-panel {
            display: none;
        }

        .symbol-panel.active {
            display: block;
        }

        /* SIGNAL HERO */
        .signal-hero {
            background: var(--bg-card);
            border: 2px solid var(--border);
            border-radius: 15px;
            padding: 25px;
            margin-bottom: 20px;
            display: grid;
            grid-template-columns: 200px 1fr 250px;
            gap: 25px;
            align-items: center;
            position: relative;
            overflow: hidden;
        }

        .signal-hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
            background: linear-gradient(90deg, var(--border), var(--border));
            transition: background 0.5s;
        }

        .signal-hero.buy::before { background: linear-gradient(90deg, var(--green), var(--green-dim)); }
        .signal-hero.sell::before { background: linear-gradient(90deg, var(--red), var(--red-dim)); }

        .signal-action-box {
            text-align: center;
        }

        .signal-label {
            font-size: 11px;
            color: var(--text-dim);
            text-transform: uppercase;
            letter-spacing: 2px;
            margin-bottom: 8px;
        }

        .signal-action {
            font-family: 'Orbitron', sans-serif;
            font-size: 48px;
            font-weight: 900;
            line-height: 1;
            transition: color 0.3s;
        }

        .signal-action.buy { color: var(--green); text-shadow: 0 0 30px rgba(0,255,136,0.5); }
        .signal-action.sell { color: var(--red); text-shadow: 0 0 30px rgba(255,68,85,0.5); }
        .signal-action.wait { color: var(--text-dim); }

        .signal-confidence {
            font-family: 'Orbitron', sans-serif;
            font-size: 24px;
            margin-top: 8px;
            color: var(--blue);
        }

        .signal-reasoning {
            padding: 15px;
            background: rgba(0,0,0,0.3);
            border-radius: 10px;
            border-left: 3px solid var(--blue);
        }

        .signal-reasoning-title {
            font-size: 10px;
            color: var(--text-dim);
            text-transform: uppercase;
            letter-spacing: 2px;
            margin-bottom: 8px;
        }

        .signal-reasoning-text {
            font-size: 13px;
            line-height: 1.5;
            color: var(--text);
        }

        .signal-meta {
            display: flex;
            flex-direction: column;
            gap: 10px;
        }

        .signal-meta-item {
            display: flex;
            justify-content: space-between;
            padding: 10px 12px;
            background: rgba(0,0,0,0.3);
            border-radius: 8px;
        }

        .signal-meta-label {
            font-size: 10px;
            color: var(--text-dim);
            text-transform: uppercase;
        }

        .signal-meta-value {
            font-family: 'Orbitron', sans-serif;
            font-size: 14px;
            font-weight: 700;
        }

        /* STATS GRID */
        .stats-grid {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 15px;
            margin-bottom: 20px;
        }

        .stat-card {
            background: var(--bg-card);
            border: 1px solid var(--border);
            border-radius: 10px;
            padding: 15px;
            text-align: center;
        }

        .stat-label {
            font-size: 10px;
            color: var(--text-dim);
            text-transform: uppercase;
            letter-spacing: 1px;
            margin-bottom: 8px;
        }

        .stat-value {
            font-family: 'Orbitron', sans-serif;
            font-size: 22px;
            font-weight: 700;
        }

        .stat-value.green { color: var(--green); }
        .stat-value.red { color: var(--red); }
        .stat-value.blue { color: var(--blue); }
        .stat-value.yellow { color: var(--yellow); }

        /* SPOOFING DETAILS */
        .spoofing-details {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
            margin-bottom: 20px;
        }

        .spoofing-side {
            background: var(--bg-card);
            border: 1px solid var(--border);
            border-radius: 12px;
            padding: 20px;
        }

        .spoofing-side.ask {
            border-left: 4px solid var(--red);
        }

        .spoofing-side.bid {
            border-left: 4px solid var(--green);
        }

        .spoofing-title {
            font-size: 14px;
            font-weight: 700;
            margin-bottom: 15px;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .spoofing-title.ask { color: var(--red); }
        .spoofing-title.bid { color: var(--green); }

        .spoofing-stats {
            display: flex;
            gap: 20px;
        }

        .spoofing-stat {
            flex: 1;
        }

        .spoofing-stat-label {
            font-size: 10px;
            color: var(--text-dim);
            text-transform: uppercase;
        }

        .spoofing-stat-value {
            font-family: 'Orbitron', sans-serif;
            font-size: 24px;
            font-weight: 700;
            margin-top: 5px;
        }

        /* ALERTS LIST */
        .alerts-section {
            background: var(--bg-card);
            border: 1px solid var(--border);
            border-radius: 12px;
            padding: 20px;
        }

        .alerts-title {
            font-size: 14px;
            font-weight: 700;
            margin-bottom: 15px;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .alerts-list {
            max-height: 300px;
            overflow-y: auto;
        }

        .alert-item {
            padding: 10px 15px;
            background: rgba(0,0,0,0.3);
            border-radius: 8px;
            margin-bottom: 8px;
            display: flex;
            justify-content: space-between;
            align-items: center;
            font-size: 12px;
        }

        .alert-item.ask { border-left: 3px solid var(--red); }
        .alert-item.bid { border-left: 3px solid var(--green); }

        .alert-price {
            font-family: 'Orbitron', sans-serif;
            font-weight: 700;
        }

        .alert-size {
            color: var(--text-dim);
        }

        .alert-confidence {
            padding: 3px 8px;
            border-radius: 5px;
            font-size: 10px;
            font-weight: 700;
        }

        .alert-confidence.high { background: rgba(255,68,85,0.3); color: var(--red); }
        .alert-confidence.medium { background: rgba(255,170,0,0.3); color: var(--yellow); }

        .no-alerts {
            text-align: center;
            padding: 30px;
            color: var(--text-dim);
        }

        /* OVERVIEW GRID (All symbols at once) */
        .overview-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 15px;
            margin-bottom: 20px;
        }

        .overview-card {
            background: var(--bg-card);
            border: 1px solid var(--border);
            border-radius: 12px;
            padding: 20px;
            cursor: pointer;
            transition: all 0.3s;
        }

        .overview-card:hover {
            border-color: var(--blue);
            transform: translateY(-2px);
        }

        .overview-card.buy { border-top: 3px solid var(--green); }
        .overview-card.sell { border-top: 3px solid var(--red); }
        .overview-card.wait { border-top: 3px solid var(--text-dim); }

        .overview-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 15px;
        }

        .overview-symbol {
            font-family: 'Orbitron', sans-serif;
            font-size: 18px;
            font-weight: 700;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .overview-signal {
            font-family: 'Orbitron', sans-serif;
            font-size: 24px;
            font-weight: 900;
        }

        .overview-signal.buy { color: var(--green); }
        .overview-signal.sell { color: var(--red); }
        .overview-signal.wait { color: var(--text-dim); }

        .overview-details {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 10px;
        }

        .overview-detail {
            padding: 10px;
            background: rgba(0,0,0,0.3);
            border-radius: 8px;
        }

        .overview-detail-label {
            font-size: 9px;
            color: var(--text-dim);
            text-transform: uppercase;
        }

        .overview-detail-value {
            font-family: 'Orbitron', sans-serif;
            font-size: 14px;
            font-weight: 700;
            margin-top: 3px;
        }

        /* Loading */
        .loading {
            text-align: center;
            padding: 50px;
            color: var(--text-dim);
        }

        .loading-spinner {
            width: 40px;
            height: 40px;
            border: 3px solid var(--border);
            border-top-color: var(--green);
            border-radius: 50%;
            animation: spin 1s linear infinite;
            margin: 0 auto 15px;
        }

        @keyframes spin {
            to { transform: rotate(360deg); }
        }

        /* Responsive */
        @media (max-width: 1200px) {
            .signal-hero {
                grid-template-columns: 1fr;
            }
            .stats-grid {
                grid-template-columns: repeat(2, 1fr);
            }
            .spoofing-details {
                grid-template-columns: 1fr;
            }
        }
    </style>
</head>
<body>
    <!-- HEADER -->
    <div class="header">
        <div class="logo">
            <div class="logo-icon">ðŸŽ¯</div>
            <div>
                <div class="logo-text">MULTI-SYMBOL TRACKER</div>
                <div class="logo-subtitle">ANTI-SPOOFING â€¢ REAL-TIME</div>
            </div>
        </div>
        <div class="nav-links">
            <a href="/antispoof" class="nav-link">Single Symbol</a>
            <a href="/multisymbol" class="nav-link active">Multi Symbol</a>
            <a href="/dashboard" class="nav-link">Dashboard</a>
            <a href="/trades" class="nav-link">Trades</a>
        </div>
    </div>

    <!-- TABS -->
    <div class="tabs-container">
        <div class="tab active" data-symbol="ALL" onclick="switchTab('ALL')">
            <span>ðŸ“Š</span>
            <span>OVERVIEW</span>
        </div>
        <% symbols.forEach(function(symbol) { %>
            <div class="tab" data-symbol="<%= symbol %>" onclick="switchTab('<%= symbol %>')">
                <div class="tab-icon <%= symbol.split('-')[0].toLowerCase() %>">
                    <%= symbol.split('-')[0].charAt(0) %>
                </div>
                <span><%= symbol.replace('-USDC', '') %></span>
                <span class="tab-signal wait" id="tab-signal-<%= symbol %>">--</span>
            </div>
        <% }); %>
    </div>

    <!-- MAIN CONTAINER -->
    <div class="container">
        <!-- OVERVIEW PANEL (All symbols) -->
        <div class="symbol-panel active" id="panel-ALL">
            <h2 style="margin-bottom: 20px; font-family: 'Orbitron', sans-serif;">ðŸ“Š All Symbols Overview</h2>
            <div class="overview-grid" id="overview-grid">
                <div class="loading">
                    <div class="loading-spinner"></div>
                    <div>Loading symbols...</div>
                </div>
            </div>
        </div>

        <!-- INDIVIDUAL SYMBOL PANELS -->
        <% symbols.forEach(function(symbol) { %>
            <div class="symbol-panel" id="panel-<%= symbol %>">
                <!-- Signal Hero -->
                <div class="signal-hero" id="hero-<%= symbol %>">
                    <div class="signal-action-box">
                        <div class="signal-label">SEGNALE <%= symbol.replace('-USDC', '') %></div>
                        <div class="signal-action wait" id="action-<%= symbol %>">WAIT</div>
                        <div class="signal-confidence" id="conf-<%= symbol %>">0%</div>
                    </div>
                    <div class="signal-reasoning">
                        <div class="signal-reasoning-title">Reasoning</div>
                        <div class="signal-reasoning-text" id="reason-<%= symbol %>">Waiting for data...</div>
                    </div>
                    <div class="signal-meta">
                        <div class="signal-meta-item">
                            <span class="signal-meta-label">Price</span>
                            <span class="signal-meta-value" id="price-<%= symbol %>">$--</span>
                        </div>
                        <div class="signal-meta-item">
                            <span class="signal-meta-label">Dominance</span>
                            <span class="signal-meta-value" id="dom-<%= symbol %>">--%</span>
                        </div>
                        <div class="signal-meta-item">
                            <span class="signal-meta-label">Alerts</span>
                            <span class="signal-meta-value" id="alerts-count-<%= symbol %>">0</span>
                        </div>
                    </div>
                </div>

                <!-- Stats Grid -->
                <div class="stats-grid">
                    <div class="stat-card">
                        <div class="stat-label">ASK Spoofing</div>
                        <div class="stat-value red" id="ask-count-<%= symbol %>">0</div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-label">ASK Volume</div>
                        <div class="stat-value red" id="ask-vol-<%= symbol %>">0</div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-label">BID Spoofing</div>
                        <div class="stat-value green" id="bid-count-<%= symbol %>">0</div>
                    </div>
                    <div class="stat-card">
                        <div class="stat-label">BID Volume</div>
                        <div class="stat-value green" id="bid-vol-<%= symbol %>">0</div>
                    </div>
                </div>

                <!-- Spoofing Details -->
                <div class="spoofing-details">
                    <div class="spoofing-side ask">
                        <div class="spoofing-title ask">ðŸ”´ ASK Spoofing (Fake Sell Walls)</div>
                        <div class="spoofing-stats">
                            <div class="spoofing-stat">
                                <div class="spoofing-stat-label">Count</div>
                                <div class="spoofing-stat-value" id="ask-detail-count-<%= symbol %>" style="color: var(--red);">0</div>
                            </div>
                            <div class="spoofing-stat">
                                <div class="spoofing-stat-label">Volume</div>
                                <div class="spoofing-stat-value" id="ask-detail-vol-<%= symbol %>" style="color: var(--red);">0</div>
                            </div>
                        </div>
                    </div>
                    <div class="spoofing-side bid">
                        <div class="spoofing-title bid">ðŸŸ¢ BID Spoofing (Fake Buy Walls)</div>
                        <div class="spoofing-stats">
                            <div class="spoofing-stat">
                                <div class="spoofing-stat-label">Count</div>
                                <div class="spoofing-stat-value" id="bid-detail-count-<%= symbol %>" style="color: var(--green);">0</div>
                            </div>
                            <div class="spoofing-stat">
                                <div class="spoofing-stat-label">Volume</div>
                                <div class="spoofing-stat-value" id="bid-detail-vol-<%= symbol %>" style="color: var(--green);">0</div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Alerts List -->
                <div class="alerts-section">
                    <div class="alerts-title">ðŸš¨ Recent Spoofing Alerts</div>
                    <div class="alerts-list" id="alerts-list-<%= symbol %>">
                        <div class="no-alerts">No alerts yet</div>
                    </div>
                </div>
            </div>
        <% }); %>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script>
        const socket = io();
        const symbols = <%- JSON.stringify(symbols) %>;
        let currentTab = 'ALL';
        let symbolData = {};

        // Initialize data structure
        symbols.forEach(sym => {
            symbolData[sym] = {
                signal: { action: 'WAIT', confidence: 0, reasoning: '', details: {} },
                alerts: []
            };
        });

        function switchTab(symbol) {
            // Update tab states
            document.querySelectorAll('.tab').forEach(tab => {
                tab.classList.remove('active');
                if (tab.dataset.symbol === symbol) {
                    tab.classList.add('active');
                }
            });

            // Update panel visibility
            document.querySelectorAll('.symbol-panel').forEach(panel => {
                panel.classList.remove('active');
            });
            document.getElementById(`panel-${symbol}`).classList.add('active');

            currentTab = symbol;
        }

        function updateSymbolUI(symbol, data) {
            if (!data) return;

            const signal = data.signal || { action: 'WAIT', confidence: 0, details: {} };
            const action = signal.action.toLowerCase();

            // Update tab signal indicator
            const tabSignal = document.getElementById(`tab-signal-${symbol}`);
            if (tabSignal) {
                tabSignal.textContent = signal.action;
                tabSignal.className = `tab-signal ${action}`;
            }

            // Update hero section
            const hero = document.getElementById(`hero-${symbol}`);
            if (hero) {
                hero.className = `signal-hero ${action}`;
            }

            const actionEl = document.getElementById(`action-${symbol}`);
            if (actionEl) {
                actionEl.textContent = signal.action;
                actionEl.className = `signal-action ${action}`;
            }

            const confEl = document.getElementById(`conf-${symbol}`);
            if (confEl) {
                confEl.textContent = `${signal.confidence}%`;
            }

            const reasonEl = document.getElementById(`reason-${symbol}`);
            if (reasonEl) {
                reasonEl.textContent = signal.reasoning || 'No data';
            }

            // Update meta
            const details = signal.details || {};
            
            const priceEl = document.getElementById(`price-${symbol}`);
            if (priceEl && details.currentPrice) {
                priceEl.textContent = `$${details.currentPrice.toFixed(2)}`;
            }

            const domEl = document.getElementById(`dom-${symbol}`);
            if (domEl && details.spoofRatio !== undefined) {
                domEl.textContent = `${(details.spoofRatio * 100).toFixed(0)}%`;
            }

            const alertsCountEl = document.getElementById(`alerts-count-${symbol}`);
            if (alertsCountEl && details.highConfidenceAlerts !== undefined) {
                alertsCountEl.textContent = details.highConfidenceAlerts;
            }

            // Update stats
            document.getElementById(`ask-count-${symbol}`).textContent = details.askSpoofCount || 0;
            document.getElementById(`ask-vol-${symbol}`).textContent = (details.askSpoofVolume || 0).toFixed(4);
            document.getElementById(`bid-count-${symbol}`).textContent = details.bidSpoofCount || 0;
            document.getElementById(`bid-vol-${symbol}`).textContent = (details.bidSpoofVolume || 0).toFixed(4);

            // Update detail sections
            document.getElementById(`ask-detail-count-${symbol}`).textContent = details.askSpoofCount || 0;
            document.getElementById(`ask-detail-vol-${symbol}`).textContent = (details.askSpoofVolume || 0).toFixed(4);
            document.getElementById(`bid-detail-count-${symbol}`).textContent = details.bidSpoofCount || 0;
            document.getElementById(`bid-detail-vol-${symbol}`).textContent = (details.bidSpoofVolume || 0).toFixed(4);
        }

        function updateAlertsUI(symbol, alerts) {
            const alertsList = document.getElementById(`alerts-list-${symbol}`);
            if (!alertsList) return;

            if (!alerts || alerts.length === 0) {
                alertsList.innerHTML = '<div class="no-alerts">No alerts yet</div>';
                return;
            }

            // Show last 20 alerts
            const recentAlerts = alerts.slice(-20).reverse();
            alertsList.innerHTML = recentAlerts.map(alert => `
                <div class="alert-item ${alert.side.toLowerCase()}">
                    <div>
                        <span class="alert-price">$${alert.priceLevel.toFixed(2)}</span>
                        <span class="alert-size">${alert.originalSize.toFixed(4)}</span>
                    </div>
                    <span class="alert-confidence ${alert.confidence >= 85 ? 'high' : 'medium'}">${alert.confidence}%</span>
                </div>
            `).join('');
        }

        function updateOverviewGrid() {
            const grid = document.getElementById('overview-grid');
            if (!grid) return;

            grid.innerHTML = symbols.map(symbol => {
                const data = symbolData[symbol] || {};
                const signal = data.signal || { action: 'WAIT', confidence: 0, details: {} };
                const action = signal.action.toLowerCase();
                const details = signal.details || {};

                return `
                    <div class="overview-card ${action}" onclick="switchTab('${symbol}')">
                        <div class="overview-header">
                            <div class="overview-symbol">
                                <div class="tab-icon ${symbol.split('-')[0].toLowerCase()}">${symbol.split('-')[0].charAt(0)}</div>
                                ${symbol.replace('-USDC', '')}
                            </div>
                            <div class="overview-signal ${action}">${signal.action}</div>
                        </div>
                        <div class="overview-details">
                            <div class="overview-detail">
                                <div class="overview-detail-label">Confidence</div>
                                <div class="overview-detail-value" style="color: var(--blue);">${signal.confidence}%</div>
                            </div>
                            <div class="overview-detail">
                                <div class="overview-detail-label">Price</div>
                                <div class="overview-detail-value">$${(details.currentPrice || 0).toFixed(2)}</div>
                            </div>
                            <div class="overview-detail">
                                <div class="overview-detail-label">Dominance</div>
                                <div class="overview-detail-value">${((details.spoofRatio || 0) * 100).toFixed(0)}%</div>
                            </div>
                            <div class="overview-detail">
                                <div class="overview-detail-label">Alerts</div>
                                <div class="overview-detail-value">${details.highConfidenceAlerts || 0}</div>
                            </div>
                        </div>
                    </div>
                `;
            }).join('');
        }

        // Fetch all data periodically
        async function fetchAllData() {
            try {
                const response = await fetch('/api/multisymbol/all');
                const data = await response.json();

                if (data.signals) {
                    for (const symbol in data.signals) {
                        if (!symbolData[symbol]) symbolData[symbol] = {};
                        symbolData[symbol].signal = data.signals[symbol];
                        updateSymbolUI(symbol, symbolData[symbol]);
                    }
                }

                if (data.alerts) {
                    for (const symbol in data.alerts) {
                        if (!symbolData[symbol]) symbolData[symbol] = {};
                        symbolData[symbol].alerts = data.alerts[symbol];
                        updateAlertsUI(symbol, data.alerts[symbol]);
                    }
                }

                updateOverviewGrid();
            } catch (error) {
                console.error('Failed to fetch data:', error);
            }
        }

        // Initial fetch
        fetchAllData();

        // Update every 1 second
        setInterval(fetchAllData, 1000);

        // Socket events
        socket.on('connect', () => {
            console.log('Connected to WebSocket');
        });
    </script>
</body>
</html>
