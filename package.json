{
  "name": "trading-ai-agent",
  "version": "1.0.0",
  "description": "Autonomous AI-driven trading bot for Hyperliquid with OpenAI/Claude integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "web": "npm run build && node --max-old-space-size=8192 dist/index.js web",
    "trade": "npm run build && node --max-old-space-size=8192 dist/index.js trade",
    "bot": "npm run build && node dist/index.js trade",
    "start": "npm run build && node dist/index.js trade",
    "backtest": "npm run build && node dist/index.js backtest",
    "analyze": "npm run build && node dist/index.js analyze",
    "report": "npm run build && node dist/index.js report",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "trading",
    "ai",
    "hyperliquid",
    "bot",
    "automation",
    "cryptocurrency",
    "openai",
    "claude"
  ],
  "author": "Trading AI Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "hyperliquid": "^1.7.7",
    "node-schedule": "^2.1.1",
    "openai": "^6.9.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "technicalindicators": "^3.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-schedule": "^2.1.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
